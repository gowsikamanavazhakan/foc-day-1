#include <stdio.h>
#include <math.h>

int main()
{
  int Number, i, Sum = 0;

  printf("\nPlease Enter any positive integer \n");
  scanf("%d",&Number);

  Sum = pow(((Number * (Number + 1))/ 2), 2);
  
  for(i =1; i<=Number;i++)
  {
    if (i != Number)
       printf("%d^3 + ",i);
    
    else
       printf("%d^3 = %d ",i, Sum);
  }
}
